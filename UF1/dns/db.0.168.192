;
; BIND reverse data file for local loopback interface
;
$TTL	604800
0.168.192.in-addr.arpa.	IN	SOA	dns.inscastellet.cat. root.dns.inscastellet.cat. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	dns.inscastellet.cat.
50	IN	PTR	dns.inscastellet.cat.
5	IN	PTR	www.inscastellet.cat.
10	IN	PTR	mail.inscastellet.cat.
15	IN	PTR	printer.inscastellet.cat.
